  object Tabla: TIBQuery
    Database = DM.BaseDatos
    Transaction = DM.Transaccion
    SQL.Strings = (
      'SELECT *'
      ' FROM "CtaCte"'
      ' INNER JOIN ("CtaCteItem"'
      ' INNER JOIN "Cliente"'
      ' ON "CtaCteItem".CLIENTE = "Cliente".CODIGO)'
      ' ON "CtaCte".CLIENTE = "CtaCteItem".CLIENTE;')
